{
    "name": "kuxitech/tp-jwt-auth",
    "description": "thinkphp jwt auth composer,fix package version error",
    "type": "library",
    "require": {
        "php": "^7.0 || ^8.0",
        "topthink/framework": "^5.1.10 || ^6.0.0",
        "lcobucci/jwt": "~4.4.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thans",
            "email": "360641274@qq.com"
        },
        {
            "name": "Kuxi Tech",
            "email": "admin@kuxi.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "thans\\jwt\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "thans\\jwt\\Service"
            ],
            "config": {
                "jwt": "config/config.php"
            }
        }
    },
    "minimum-stability": "dev"
}
